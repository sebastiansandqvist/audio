    "browser-sync": "2.26.14",
    "npm-run-all": "4.1.5",
    "rollup": "2.44.0",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "typescript": "4.2.3"
  },
